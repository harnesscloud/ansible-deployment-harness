echo "setting harness user login variables and disabling http_proxy"
unset http_proxy
unset HTTP_PROXY
unset OS_SERVICE_TOKEN
unset OS_SERVICE_ENDPOINT
export OS_USERNAME=harness
export OS_TENANT_NAME=harness
export OS_PASSWORD={{ openstack_identity_harness_password }}
export OS_AUTH_URL={{ openstack_identity_public_url }}
export OS_REGION_NAME={{ openstack_identity_region }}
