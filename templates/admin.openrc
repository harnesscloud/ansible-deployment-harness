echo "setting admin login variables and disabling http_proxy"
unset http_proxy
unset HTTP_PROXY
unset OS_USERNAME
unset OS_TENANT_NAME
unset OS_PASSWORD
unset OS_AUTH_URL
unset OS_REGION_NAME
export OS_SERVICE_TOKEN={{ openstack_identity_admin_token }}
export OS_SERVICE_ENDPOINT={{ openstack_identity_admin_url }}
